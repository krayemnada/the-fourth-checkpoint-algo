PROCEDURE permut(a,b:INTEGER)
VAR
    temp:INTEGER;
BEGIN
   temp:=a ;
   a:=b;
   b:=temp;
END


ALGORITHM sort
VAR
    tab : ARRAY_OF INTEGER[100];
    i,j:INTEGER
BEGIN
    FOR i FROM 0 TO tab.length-1 STEP 1  DO
        j:=i;
         WHILE (j>0 AND tab[j]<tab[j-1]) DO
             permut(tab[j],tab(j-1));
             j:=j-1;
         END_WHILE
    END_FOR
END